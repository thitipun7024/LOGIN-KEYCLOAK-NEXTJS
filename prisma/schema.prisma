generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model AssetMaster {
  id            Int      @id @default(autoincrement()) @map("id")
  AssetCode     String   @map("AssetCode")
  Serial        String?  @map("Serial")
  ModelName     String   @map("ModelName")
  AssetTypeCode String?   @map("AssetTypeCode")
  AssetTypeName String?   @map("AssetTypeName")
  BranchCode    String?    @map("BranchCode")
  
  @@map("tblAssetMaster")
}
